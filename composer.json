{
    "name":                         "blueo/un-wp-plugin",
    "description":                  "Wordpress plugin for UNYouth NZ - keeps php code out of theme files",
    "authors": [
        {
            "name":                 "Bernard Hamlin"
        }
    ],
    "type": "wordpress-plugin",
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "*",
        "wordpress/wordpress" : "3.6"
    },
    "repositories" :[
        {
                "type":                 "package",
                "package": {
                    "name":             "wordpress/wordpress",
                    "version":          "3.6",
                    "dist":             {
                        "type":         "zip",
                        "url":          "https://github.com/WordPress/WordPress/zipball/3.6"
                    }
                }
        }
    ]
}